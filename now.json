{
    "version": 2,
    "builds": [
      { "src": "*.py", "use": "@liudonghua123/now-flask" }
    ],
    "alias": [
        "PakAirQuality"
      ],
      "routes": [
        { "src": "/facebook", "dest": "/facebook.py" },
        { "src": "/hello", "dest": "/hello.py" }
      ],
      "env": {
        "AIRVISUAL": "@AIRVISUAL",
        "PAGE_ACCESS_TOKEN": "@PAGE_ACCESS_TOKEN",
        "FB_CLIENT_TOKEN": "@FB_CLIENT_TOKEN",
        "VERIFY_TOKEN": "@VERIFY_TOKEN"
      }
  }